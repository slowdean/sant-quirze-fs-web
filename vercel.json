{
  "version": 2,
  "builds": [
    { "src": "api/inscripcion.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/inscripcion", "dest": "api/inscripcion.js" },
    { "src": "/(.*)\\.(css|js|png|jpg|jpeg|gif|svg|webp|ico)", "dest": "/public/$1.$2" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}

